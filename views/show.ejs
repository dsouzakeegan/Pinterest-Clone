<% include ./partials/header.ejs %>
    <div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
        <div class="cards flex items-center flex-wrap gap-10">
            <% user.posts.forEach(function(elem){ %>
                <div class="card w-40">
                    <div class="w-full h-40 bg-zinc-800 rounded-lg overflow-hidden flex items-end">
                        <img src="/images/uploads/<%= elem.image%>" alt="">
                    </div>
                    <h5 class="mt-2 text-sm"><%= elem.title %></h5>
                </div>
            <% }) %>
        </div>
    </div>
<% include ./partials/footer.ejs %>